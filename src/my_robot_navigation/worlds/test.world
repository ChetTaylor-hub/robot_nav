<?xml version="1.0"?>
<sdf version="1.6">
  <world name="default">
    <!-- Ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Sun -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- Room (a simple box) -->
    <model name="room">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>10 10 3</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>10 10 3</size>
            </box>
          </geometry>
          <material>
            <ambient>0.7 0.7 0.7 1</ambient>
            <diffuse>0.7 0.7 0.7 1</diffuse>
          </material>
        </visual>
      </link>
      <pose>0 0 1.5 0 0 0</pose>
    </model>

    <!-- Banana -->
    <include>
      <uri>model://banana</uri>
      <pose>0 1 0.1 0 0 0</pose>
    </include>

    <!-- Bottle -->
    <include>
      <uri>model://beer</uri>
      <pose>1 0 0.1 0 0 0</pose>
    </include>

    <!-- Bus -->
    <include>
      <uri>model://bus</uri>
      <pose>-1 -1 0.1 0 0 0</pose>
    </include>
  </world>
</sdf>
